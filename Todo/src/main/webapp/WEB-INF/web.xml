<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>kr.or.connect.todo.api.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TodoFromServlet</servlet-name>
  	<display-name>TodoFromServlet</display-name>
  	<description></description>
  	<servlet-class>kr.or.connect.todo.api.TodoFromServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TodoAddServlet</servlet-name>
  	<display-name>TodoAddServlet</display-name>
  	<description></description>
  	<servlet-class>kr.or.connect.todo.api.TodoAddServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/todo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoFromServlet</servlet-name>
  	<url-pattern>/todoform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TodoAddServlet</servlet-name>
  	<url-pattern>/todoadd</url-pattern>
  </servlet-mapping>
  
</web-app>
